{
  "expo": {
    "newArchEnabled": true,
    "name": "Tabby",
    "slug": "Tabby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caesarx26.tabby"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/app/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.caesarx26.tabby"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/app/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "98b62fc4-f598-4842-8362-2f8704b6c0e8"
      }
    }
  }
}
